<h1>All Users</h1>
<div class="container">
  <div class="span10 offset1">
    <table class="table table-condensed table-bordered table-striped">
      <thead>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email Address</th>
          <th>Joined</th>
          <th>Edit User</th>
      </thead>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.first_name %></td>
          <td><%= user.last_name %></td>
          <td><%= user.email %></td>
          <td><%= time_ago_in_words(user.created_at) %></td>
          <td><%= link_to "Edit", "#" %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>