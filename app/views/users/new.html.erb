<h1>Create an Account</h1>
<div class="row">
  <div class="span6 offset4">
    <%= form_for @user do |f| %>
        <%= render 'shared/errors', object: f.object %>
        <%= render 'fields', f: f %>

        <%= f.submit "Create My Account", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>